<h1>All Heroines</h1>

<h2>Search by superpower:</h2>
<%= form_with url: search_path do |f| %>
  <%= f.label :power %>
  <%= f.text_field :power %>
  <%= f.submit "Search" %>
<% end %>

<ul>
  <% @heroines.each do |heroine| %>
    <li>
      <%= link_to heroine.super_name, heroine %>
    </li>
    <br>
  <% end %>
</ul>
